<resources>
    <style name="CarbonTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textColorPrimary">#61615E</item>
        <item name="android:textColorPrimaryInverse">@android:color/white</item>
        <item name="android:selectableItemBackground">@drawable/selectable_item_background_compat</item>
    </style>

    <style name="CarbonThemeDark" parent="ThemeOverlay.AppCompat.Dark">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="BackupOverFlow">
        <item name="android:src">@drawable/ic_action_core_overflow</item>
    </style>

    <style name="BackupRestoreTheme" parent="@style/CarbonTheme">
    </style>
</resources>